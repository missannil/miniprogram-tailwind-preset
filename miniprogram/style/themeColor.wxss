.light {
	/* background-color*/
	/* 主题色 */
	--bg-primary-darkest: rgba(21, 128, 61);
	--bg-primary-dark: rgba(22, 163, 74);
	--bg-primary: rgba(29, 228, 102);
	--bg-primary-light: rgba(74, 222, 128);
	--bg-primary-lightest: rgba(134, 239, 172);
	/* 醒目色 */
	--bg-striking-darkest: rgba(185, 28, 28);
	--bg-striking-dark: rgba(220, 38, 38);
	--bg-striking: rgba(254, 68, 68);
	--bg-striking-light: rgba(248, 113, 113);
	--bg-striking-lightest: rgba(252, 166, 166);
	/* 辅助色 */
	--bg-embellish-darkest: rgba(29, 78, 216);
	--bg-embellish-dark: rgba(37, 99, 235);
	--bg-embellish: rgba(59, 130, 246);
	--bg-embellish-light: rgba(96, 165, 250);
	--bg-embellish-lightest: rgba(147, 197, 253);
	/* 灰色 */
	--bg-gray-darkest: rgba(55, 65, 81);
	--bg-gray-dark: rgba(75, 85, 99);
	--bg-gray: rgba(107, 114, 128);
	--bg-gray-light: rgba(156, 163, 175);
	--bg-gray-lightest: rgba(209, 213, 219);
	/* 白色 */
	--bg-white-darkest: rgba(212, 212, 212);
	--bg-white-dark: rgba(229, 229, 229);
	--bg-white: rgba(245, 245, 245);
	--bg-white-light: rgba(250, 250, 250);
	--bg-white-lightest: rgba(255, 255, 255);
	/* color(字体)*/
	/* 主题色 */
	--text-primary-darkest: rgba(21, 128, 61);
	--text-primary-dark: rgba(22, 163, 74);
	--text-primary: rgba(29, 228, 102);
	--text-primary-light: rgba(74, 222, 128);
	--text-primary-lightest: rgba(134, 239, 172);
	/* 醒目色 */
	--text-striking-darkest: rgba(185, 28, 28);
	--text-striking-dark: rgba(220, 38, 38);
	--text-striking: rgba(254, 68, 68);
	--text-striking-light: rgba(248, 113, 113);
	--text-striking-lightest: rgba(252, 166, 166);
	/* 辅助色 */
	--text-embellish-darkest: rgba(29, 78, 216);
	--text-embellish-dark: rgba(37, 99, 235);
	--text-embellish: rgba(59, 130, 246);
	--text-embellish-light: rgba(96, 165, 250);
	--text-embellish-lightest: rgba(147, 197, 253);
	/* 灰色 */
	--text-gray-darkest: rgba(55, 65, 81);
	--text-gray-dark: rgba(75, 85, 99);
	--text-gray: rgba(107, 114, 128);
	--text-gray-light: rgba(156, 163, 175);
	--text-gray-lightest: rgba(209, 213, 219);
	/* 白色 */
	--text-white-darkest: rgba(212, 212, 212);
	--text-white-dark: rgba(229, 229, 229);
	--text-white: rgba(245, 245, 245);
	--text-white-light: rgba(250, 250, 250);
	--text-white-lightest: rgba(255, 255, 255);
}
