view,[class] {
  box-sizing: border-box
}

.m-20 {
  margin: 20rpx
}

.flex {
  display: flex
}

.hidden {
  display: none
}

.h-10vh {
  height: 10vh
}

.h-10vw {
  height: 10vw
}

.h-50 {
  height: 50rpx
}

.h-5vw {
  height: 5vw
}

.w-100 {
  width: 100rpx
}

.w-1F2 {
  width: 50%
}

.w-1F3 {
  width: 33.333333%
}

.w-200 {
  width: 200rpx
}

.w-full {
  width: 100%
}

.flex-1 {
  flex: 1 1 0%
}

.flex-none {
  flex: none
}

.overflow-hidden {
  overflow: hidden
}

.p-50 {
  padding: 50rpx
}

.text-40 {
  font-size: 40rpx
}

.font-bold {
  font-weight: 700
}

.italic {
  font-style: italic
}

.antialiased {
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale
}

.subpixel-antialiased {
  -webkit-font-smoothing: auto;
  -moz-osx-font-smoothing: auto
}