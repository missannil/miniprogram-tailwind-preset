{
	"compilerOptions": {
		"lib": ["ESNext"],
		"target": "ES2022",
		"module": "ESNext",
		"strict": true,
		"exactOptionalPropertyTypes": true,
		"skipLibCheck": true,
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"diagnostics": true,
		"declaration": true,
		"removeComments": true,
		"noImplicitReturns": true,
		"forceConsistentCasingInFileNames": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"verbatimModuleSyntax": true,
	},
	"include": ["index.ts"]
}
