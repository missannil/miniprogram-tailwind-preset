  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.3",
    "commitlint": "^17.6.3",
    "husky": "^8.0.3",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "src/",
    "README.md",
    "CHANGELOG.md"
  ]
}
